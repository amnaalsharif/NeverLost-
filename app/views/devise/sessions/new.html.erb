<div class="panel panel-default devise-bs">
  <div class="panel-heading">
    <div id="blue"><%= t('.sign_in', :default => "Sign in") %></div>
  </div>
  <div class="panel-body">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { role: "form" }) do |f| %>
      <div class="form-group">
     <div id="tablefont">   <%= f.label :email %> </div>
        <%= f.email_field :email, autofocus: true, class: "form-control" %>
      </div>
      <div class="form-group">
     <div id="tablefont">   <%= f.label :password %> </div>
        <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
      </div>
      <% if devise_mapping.rememberable? %>
        <div class="checkbox">
        <%= f.label :remember_me do %> 
          <div id="tablefont"> <%= f.check_box :remember_me %> <%= t('.remember_me', :defaul=> "Remember me") %> </div>
          <% end %>
        </div>
      <% end %>
      <%= f.submit  t('.sign_in', :default => "Sign in"), class: "btn btn-large btn-info" %>
    <% end %>
  </div>
</div>
<%= render "devise/shared/links" %>
